{
  "title": "Platform.sh Documentation",
  "root": "src",
  "gitbook": ">=3.2",
  "language": "en",
  "plugins": [
    "anchors",
    "reveal",
    "collapsible-menu",
    "codesnippet",
    "ga",
    "edit-link"
  ],
  "pluginsConfig": {
    "edit-link": {
      "base": "https://github.com/platformsh/platformsh-docs/edit/master/src",
      "label": "Edit Page"
    },
    "ga": {
      "token": "UA-4064131-10"
    },
    "sharing": {
      "google": false,
      "facebook": false,
      "twitter": false,
      "share": false,
      "all": [
        "facebook", "google", "twitter", "share"
      ]
    }
  }
}
